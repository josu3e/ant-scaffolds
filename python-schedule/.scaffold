base:
  name: 'pythonSCHEDULE'
  description: 'Proyecto para la creaci√≥n de schedules (crones) en el lenguaje Python'
  scaffold_name: 'python-schedule'
  save_prompts: 'rubick_prompts.json'
  prompts:
    -
      name: 'command_name'
      description: 'Ingrese el nombre de su comando'
      default: 'example'
    -
      name: 'domain'
      description: 'Ingrese el nombre de dominio de su producto'
      default: 'mydomain.com'
  replace_names:
    -
      search: '+owner+'
      use_prompt: 'product_name'
    -
      search: '+command_name+'
      use_prompt: 'command_name'