scaffold:
  author: 'DevLusaja'
  name: 'python-schedule'
  description: 'Python schedule using click library'
  save_prompts: 'prompts.json'
  prompts:
    -
      name: 'product_name'
      description: 'Enter the product name [urbania, neo, aptitus, pagoefectivo]'
      default: 'orbis'
    -
      name: 'command_name'
      description: 'enter the command name'
      default: 'example'
    -
      name: 'domain'
      description: 'Enter the product domain'
      default: 'domain.com'
  replace_names:
    -
      search: '+owner+'
      use_prompt: 'product_name'
    -
      search: '+command_name+'
      use_prompt: 'command_name'