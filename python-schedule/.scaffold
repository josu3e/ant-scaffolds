scaffold:
  author: 'DevLusaja'
  name: 'python-schedule'
  description: 'Proyecto para la creaci√≥n de schedules (crones) en el lenguaje Python'
  save_prompts: 'rubick_prompts.json'
  prompts:
    -
      name: 'command_name'
      description: 'Ingrese el nombre de su comando'
      default: 'example'
    -
      name: 'domain'
      description: 'Ingrese el nombre de su dominio'
      default: 'mydomain.com'
    -
      name: 'product_name'
      description: 'Ingrese el nombre de su producto'
      default: 'product'
  replace_names:
    -
      search: '+owner+'
      use_prompt: 'product_name'
    -
      search: '+command_name+'
      use_prompt: 'command_name'